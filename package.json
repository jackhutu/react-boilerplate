{
  "name": "react-boilerplate",
  "version": "0.0.1",
  "description": "react boilerplate",
  "repository": "https://github.com/jackhutu/react-boilerplate",
  "license": "MIT",
  "scripts": {
    "start": "npm-run-all --sequential build start:dist",
    "dev": "webpack-dev-server --env.development --mode=development --open --progress --profile --watch --color",
    "build": "npm-run-all clean --parallel compile",
    "compile": "cross-env NODE_ENV=production webpack --env.production --mode=production --progress --profile --bail --color",
    "start:dist": "cross-env NODE_ENV=production nodemon ./server.js",
    "clean": "rimraf dist"
  },
  "bugs": {
    "url": "https://github.com/jackhutu/react-boilerplate/issues"
  },
